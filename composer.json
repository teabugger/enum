{
  "name": "teabugger/enum",
  "type": "library",
  "description": "PHP ENUM implementation",
  "keywords": [
    "teabugger",
    "enum"
  ],
  "homepage": "https://github.com/teabugger/enum",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Kolesnik",
      "email": "makckolesnik@gmail.com",
      "homepage": "https://github.com/teabugger",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.45",
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Teabugger\\Enum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Teabugger\\Enum\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
